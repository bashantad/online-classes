<p id="notice"><%= notice %></p>

<h1>Qualifications</h1>

<table>
  <thead>
    <tr>
      <th>Name of institution</th>
      <th>String</th>
      <th>Year start</th>
      <th>Year end</th>
      <th>Title</th>
      <th>Location</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qualifications.each do |qualification| %>
      <tr>
        <td><%= qualification.name_of_institution %></td>
        <td><%= qualification.string %></td>
        <td><%= qualification.year_start %></td>
        <td><%= qualification.year_end %></td>
        <td><%= qualification.title %></td>
        <td><%= qualification.location %></td>
        <td><%= qualification.country %></td>
        <td><%= link_to 'Show', qualification %></td>
        <td><%= link_to 'Edit', edit_qualification_path(qualification) %></td>
        <td><%= link_to 'Destroy', qualification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Qualification', new_qualification_path %>
